{
    "manifest_version": 3,
    "name": "SoWeSketch",
    "description": "Une extention Chrome pour signer avec des images sur SoWeSign",
    "version": "0.1",
    "icons": {
        "128": "images/logo128.jpg"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "http://app.sowesign.com/*",
                "https://app.sowesign.com/*"
            ]
        }
    ]
}